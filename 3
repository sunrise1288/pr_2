import java.util.Scanner;
class Main
{
public static void main(String[] args)
{
Scanner scanner = new Scanner(System.in);
System.out.print("x = ");
double x = scanner.nextDouble();
System.out.print("y = ");
double y = scanner.nextDouble();
if (y != 0)
{
double z = x/y;
System.out.print(z);
}
else
System.out.print("Error");
}
}
