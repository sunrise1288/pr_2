import java.util.Scanner;
class Main
{
public static void main(String[] args)
{
Scanner scanner = new Scanner(System.in);
System.out.print("x = ");
int x = scanner.nextInt();
System.out.print("y = ");
int y = scanner.nextInt();
System.out.print("z = ");
int z = scanner.nextInt();
if (x>y && y>z)
System.out.print(x);
else if (y>x && x>z)
System.out.print(y);
else
System.out.print(z);
}
}
